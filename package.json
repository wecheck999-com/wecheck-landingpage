    "formik": "^2.2.9",
    "framer-motion": "^6.3.10",
    "googleapis": "^104.0.0",
    "husky": "^8.0.1",
    "next": "12.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.0.0",
    "next-seo": "^5.4.0",
    "next-svgr": "^0.0.2",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scroll-to-top": "^1.0.8",
    "react-toastify": "^8.1.0",
