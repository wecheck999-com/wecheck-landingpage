{
  "name": "landing-page",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development next dev",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "dev:production": "env-cmd -f .env.production next dev",
    "build": "next build",
